{
  "version": 2,
  "name": "medici-backend",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "DB_SERVER": "@db_server",
    "DB_DATABASE": "@db_database",
    "DB_USER": "@db_user",
    "DB_PASSWORD": "@db_password",
    "DB_PORT": "1433",
    "JWT_SECRET": "@jwt_secret",
    "NODE_ENV": "production"
  }
}
